NAME=$1
NAME="${NAME:=MyStatefulWidget}"
echo "class ${NAME} extends StatefulWidget {"
echo "  const ${NAME}({super.key});"
echo ""
echo "  @override"
echo "  State<${NAME}> createState() => _${NAME}State();"
echo "}"
echo ""
echo "class _${NAME}State extends State<${NAME}> {"
echo "  @override"
echo "  Widget build(BuildContext context) {"
echo "    return Container();"
echo "  }"
echo "}"
